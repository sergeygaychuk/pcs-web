<div class="top">
  <div class="rel">
    <div class="border"></div>
    <div class="logo">
      <a href="http://asutp.io/" title="asutp.io">asutp.io</a></div>
    <div class="search">
      <div class="sg hide">
        <div class="sf">
          <div class="rel">
            <div class="sbx">
            </div>
            <div class="so"><span class="arrow-down"></span>
            </div>
          </div>
        </div>
