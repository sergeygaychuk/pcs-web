            <tbody>
              <tr>
                <td class="check">
                  <div class="check"></div>
                </td>
                <td><!-- LED icon goes here --></td>
                <td class="sender">
                  <span>Название объекта</span>
                </td>
                <td></td>
                <td>
                  <span>Состояние</span>
                  <span class="details"> - пояснение</span>
                </td>
                <td>
                </td>
                <td class="date">
                  <span>время/дата</span>
                </td>
              </tr>
            </tbody>
          </table>
          <div class="reflow"></div>
        </div>
        <div class="footer">
          <div class="left">&nbsp;
          </div>
          <div class="center">
