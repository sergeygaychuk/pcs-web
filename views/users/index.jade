extends ../_main

block content
  .tp-data
    table.tp-data
      colgroup
        col.tp-check
        col.col-xs-6.col-sm-4.col-xl-3
        col.col-xs-4.col-sm-3.col-xl-2
        col.tp-text
      tbody
        each u in users
          tr
            td.tp-check
              .tp-check
            td.tp-sender
              a(href='/users/'+u._id)= u.name
            td.tp-email
              = u.email
            td.tp-info

